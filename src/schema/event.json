{
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "_id": {
            "type": "string",
            "required": true
        },
        "_rev": {
            "type": "string"
        },
        "_revisions":{
            "type": "object"
        },
        "schema": {
            "type": "string",
            "required": true
        },
        "title": {
            "type": "string",
            "required": true,
            "description": "Headline for event, shown above the start and end times."
        },
       "category": {
            "type": "string",
            "required": true
        },
        "start": {
            "type": "string",
            "required": true,
            "description": "Date and time the event starts."
        },
        "end": {
            "type": "string",
            "required": true,
            "description": "Date and time the event ends. After this date, the event will be removed from the website."
        },
         "custom": {
            "type": "string",
            "description": "A custom string to use for the display date instead of '<start date> from <start time>to <end time>'"
        },
        "content": {
            "type": "string",
            "required": true,
            "description": "Description of event, shown below the title and date."
        },
       "location": {
            "type": "string",
            "description": "Location of the event."
        },
       "created": {
            "type": "string",
            "required": true
        },
        "modified": {
            "type": "string"
        },
        "author": {
            "type": "string",
            "required": true
        }
    }
}
